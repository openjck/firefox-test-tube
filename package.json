{
  "name": "test-tube",
  "private": true,
  "proxy": "http://localhost:8000",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:js": "react-app-rewired build",
    "build:css": "stylus --sourcemap ./src/components/views/styl/* --out ./src/components/views/css/",
    "start": "npm-run-all --parallel start:js watch:css",
    "start:js": "react-app-rewired start",
    "watch:css": "stylus --watch --sourcemap src/components/views/styl/* --out ./src/components/views/css/",
    "eject": "react-scripts eject",
    "test": "react-app-rewired test --env=jsdom",
    "postinstall": "yarn run build"
  },
  "dependencies": {
    "antd": "^3.1.4",
    "babel-plugin-import": "^1.6.3",
    "chart.js": "^2.7.1",
    "dateformat": "^3.0.2",
    "gravatar": "^1.6.0",
    "npm-run-all": "^4.1.1",
    "photon-ant": "^0.1.4",
    "query-string": "^5.0.0",
    "react": "^16.0.0",
    "react-app-rewire-less": "^2.1.0",
    "react-app-rewired": "^1.4.0",
    "react-chartjs-2": "^2.6.4",
    "react-dom": "^16.0.0",
    "react-fa": "^5.0.0",
    "react-paginate": "^5.0.0",
    "react-refetch": "^1.0.3",
    "react-router-dom": "^4.2.2",
    "react-scripts": "^1.0.17",
    "sifter": "^0.5.3",
    "stylus": "^0.54.5"
  }
}
